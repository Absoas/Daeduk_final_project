<?xml version="1.0" encoding="UTF-8" ?>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>Insert title here</title>

</head>
<body>

	<!-- 건의 게시판 등록 -->
	<!-- 2.	kr.or.ddit.sboard.controller.SuggestboardInsertController -->
	<!-- 등록 -->

	<form method="post">
		<table>
			<tr>
				<th>건의사항게시판_번호</th>
				<td><div class="input-group">
						<input class="form-control" type="text" name="suggest_board_no"
							value="${suggest_board.suggest_board_no}" /><span
							class="input-group-text" class="error">${errors["suggest_board_no"]}</span>
					</div></td>
			</tr>
			<tr>
				<th>건의사항게시판_제목</th>
				<td><div class="input-group">
						<input class="form-control" type="text" name="suggest_board_title"
							value="${suggest_board.suggest_board_title}" /><span
							class="input-group-text" class="error">${errors["suggest_board_title"]}</span>
					</div></td>
			</tr>
			<tr>
				<th>건의사항게시판_내용</th>
				<td><div class="input-group">
						<input class="form-control" type="text"
							name="suggest_board_content"
							value="${suggest_board.suggest_board_content}" /><span
							class="input-group-text" class="error">${errors["suggest_board_content"]}</span>
					</div></td>
			</tr>
			<tr>
				<th>건의사항게시판_작성날짜</th>
				<td><div class="input-group">
						<input class="form-control" type="text" name="suggest_board_date"
							value="${suggest_board.suggest_board_date}" /><span
							class="input-group-text" class="error">${errors["suggest_board_date"]}</span>
					</div></td>
			</tr>
			<tr>
				<th>회원_아이디</th>
				<td><div class="input-group">
						<input class="form-control" type="text" name="mem_id"
							value="${suggest_board.mem_id}" /><span class="input-group-text"
							class="error">${errors["mem_id"]}</span>
					</div></td>
			</tr>

			<tr>
				<td><input type="submit" value="저장" /></td>
			</tr>
		</table>
	</form>


</body>
</html>
